#include<studio.h>
void main
{
  int n,sum=0;
  printf ("enter the number of elements in the array:");
  scanf ("%d",&n);
  int arr[n];
  printf ("enter %d elements:|n",n);
  for(int i=0; i<n ;i++)
  {
      scanf("%d",&arr[i]);
      for (int i=0; i<n;i++)
      {
          if((i+1)% 2!=0)
         {
             sum+=arr[i];
         }
      }
      printf("sum of elements at odd position:% d|n"sum);
      return 0;
}
